<?xml version="1.0" ?>
<launch>
    <env name="ROSCONSOLE_FORMAT" value="[${function}] ${message}" />
    <env name="ROSCONSOLE_CONFIG_FILE" value="$(find robotont_driver)/config/console.conf" />

    <node name="laserscan_kinect" pkg="laserscan_kinect" type="laserscan_kinect" >
        <remap from="image" to="/camera/depth/image_rect_raw"/>
        <remap from="camera_info" to="/camera/depth/camera_info"/>
        <rosparam command="load" file="$(find robotont_description)/config/laser_params.yaml" />
    </node>
    <node pkg="robotont_demos" name="laserscan_to_distance" type="laserscan_to_distance.py" output="screen" />
</launch>
